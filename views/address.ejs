<%- include("partials/header"); -%>

<div class="my-5">

</div>
<h3><a href="/user/edit/<%=contact[0].Contact_id%>"><%=contact[0].Fname%> <%=contact[0].Mname%> <%=contact[0].Lname%></a></h3>

<div class="mt-4 table-responsive">
  <table id="address_table" class="display">
    <!-- <table id="" class="table table-striped table-hover table-bordered table-sm"> -->

    <thead class="thead-dark">
      <tr>
        <th scope="col">Address Type</th>
        <th scope="col">Address</th>
        <th scope="col">City</th>
        <th scope="col">State</th>
        <th scope="col">Zip</th>
        <th scope="col">Edit</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
    <tbody>

      <% data.forEach(function(element) { %>
      <tr>
        <td><%= element.Address_type %></td>
        <td><%= element.Address %></td>
        <td><%= element.City %></td>
        <td><%= element.State %></td>
        <td><%= element.Zip %></td>
        <td><a href="/user/edit/<%=element.Contact_id%>">Edit</a></td>
        <td><a href="/address/delete/<%=element.Address_id%>">Delete</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>

<%- include("partials/footer"); -%>
